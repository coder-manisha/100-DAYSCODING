class ListNode:
     def __init__(self, x):
         self.val = x
         self.next = None


class Solution:

    def hasCycle(self, head: Optional[ListNode]) -> bool:
        n = head
        x = head
        while n and n.next:
            x = x.next
            n = n.next.next
            if x == n:
                return True
        return False
