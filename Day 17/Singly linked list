class Node:
    def __init__(self,val):
        self.val = val
        self.next = None
class SingleLinkedList:
    def __init__(self):
        self.root = None
    def addNode(self,val):
        n = Node(val)
        if self.root == None:
            self.root = n
            return
        temp = self.root
        while temp.next!=None:
            temp = temp.next
        temp.next = n

    def display(self):
        temp = self.root
        while temp:
            print(temp.val,end=' -> ')
            temp = temp.next
s = SingleLinkedList()
s.addNode(10)
s.addNode(104)
s.addNode(310)
s.addNode(120)
s.display()
