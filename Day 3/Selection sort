def selectionSort(arr):
    for step in range(len(arr)-1):
        min = step
        for i in range(step + 1, len(arr)):
            if arr[i] < arr[min]:
                min = i
        (arr[step], arr[min]) = (arr[min], arr[step])

data = [-2, 45, 0, 11, -9]
selectionSort(data)
print(f'Sorted list  is {data}')
